SUBDIRS = cdt graph agraph gd pathplan vmalloc sfio ast expr circogen \
	dotgen fdpgen neatogen pack twopigen common gvc ingraphs

all install clean :
	(for i in ${SUBDIRS}; do (cd $$i; make $@); done)       

distclean :
	(for i in ${SUBDIRS}; do (cd $$i; make $@); done)       
	rm -rf lib include
